      }, {
        "title": "NO1234567890",
        "description": "Type an ISIN number to get more information"
      }, {
        "title": "90k",
        "description": "90k will be understood as 90 000"
      }, {
        "title": "buyform",
        "description": "Get a buy form to fill out."
      }]
    }]
  }],
  "connectors": [],
  "composeExtensions": [],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "{{HOSTNAME}}"
  ],
  "showLoadingIndicator": true
